ruleset EarlyEdSchoolRuleSet

rule 52.02.0001
	when {SchoolCalendar}.[KindergartenScheduleCode] is in ['P','Q','R'] then
	require that {SchoolCalendar}.[GradeLevel] is in [
		'EC',
		'FA','FB','FC','FD','FE','FF','FG','FH','FI','FJ',   
		'PA','PB','PC','PD','PE','PF','PG','PH','PI','PJ',
		'PS',
		'RA','RB','RC','RD','RE','RF','RG','RH','RI','RJ',
		'SA','SB','SC','SD','SE','SF','SG','SH','SI','SJ'
		]
	else 'School calendar schedules of types P, Q, and R can only be associated with grade levels (EC, PA-PJ, PS, RA-RJ, SA-SJ, FA-FJ).'


