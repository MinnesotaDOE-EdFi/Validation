<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>ASDCS District Data Validation Portal</title>
    <link rel="SHORTCUT ICON" href="~/favicon.ico" />
    @Styles.Render("~/Content/css")
</head>
<body>
    <div class="container-fluid layout">
        <div class="row ">
            <div class="top-banner col-12">
                <img src="~/Content/Images/mdelogo450.png" class="img-fluid mdelogo" />
                <div class="app-title clear-margin">ASDCS District Data Validation Portal</div>
            </div>
        </div>
        <div class="row noprint">
            <div class="col-12">
                <nav class="navbar navbar-expand-sm prime-blue">
                    <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#theNavBar">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="theNavBar">
                        <ul class="navbar-nav">
                            <li class="nav-item">@Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "nav-link" })</li>
                            <li class="nav-item">@Html.ActionLink("Validation Reports", "Reports", "Validation", new { area = "" }, new { @class = "nav-link" })</li>
                            <li class="nav-item">@Html.ActionLink("Rule Collections", "Collections", "Rules", new { area = "" }, new { @class = "nav-link" })</li>
                            <li class="nav-item">@Html.ActionLink("Rule Editor", "Editor", "Rules", new { area = "" }, new { @class = "nav-link" })</li>
                            <li class="nav-item">@Html.ActionLink("Announcements", "Announcements", "Home", new { area = "" }, new { @class = "nav-link" })</li>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
        <div class="row layout">
            <div class="col-12">
                @RenderBody()
            </div>
        </div>
        @{var footerLinkBaseUrl = "https://Education.mn.gov"; }
        <div class="row layout layout-footer fixed-bottom">
            <div class="col-8 d-none d-sm-block">
                <div class="hfooter">Quick Links</div>
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-4 col-sm-12 d-none d-sm-block noprint">
                            <ul class="noDisc">
                                <li><a target="" mde="" href="@(footerLinkBaseUrl)/MDE/MDE058804">A-Z Subject Directory</a></li>
                                <li><a target="" href="https://bosa.mn.gov/BOSA/index.htm">Board of School Administrators</a></li>
                                <li><a href="https://mn.gov/board-of-teaching/" target="">Board of Teaching</a></li>
                                <li><a target="" href="@(footerLinkBaseUrl)/MDE/dse/safe/index.htm">Bullying Prevention</a></li>
                                <li><a href="@(footerLinkBaseUrl)/MDE/about/cal/index.htm" target="">Calendar</a></li>
                                <li><a href="@(footerLinkBaseUrl)/MDE/about/jobs/index.htm" target="">Careers</a></li>
                                <li><a href="@(footerLinkBaseUrl)/" target="">Department of Education</a></li>
                                <li><a href="@(footerLinkBaseUrl)/MDE/fam/elsprog/index.htm" target="">Early Learning (PreK)</a></li>
                            </ul>
                        </div>
                        <div class="col-md-4 col-sm-12 d-none d-sm-block noprint">
                            <ul class="noDisc">
                                <li><a target="" href="@(footerLinkBaseUrl)/MDE/dse/ESSA/index.htm">Every Student Succeeds Act (ESSA)</a></li>
                                <li><a target="" href="@(footerLinkBaseUrl)/MDE/dse/FNS/index.htm">Food and Nutrition Services</a></li>
                                <li><a target="" href="@(footerLinkBaseUrl)/mde/dse/grants/">Grants</a></li>
                                <li><a target="" href="@(footerLinkBaseUrl)/MDE/about/rule/index.htm">Legislation, Rulemaking and Reports</a></li>
                                <li><a target="" href="@(footerLinkBaseUrl)/MDE/Data/Maps/index.html">Maps</a></li>
                                <li><a target="" href="@(footerLinkBaseUrl)/MDE/fam/sped/index.htm">Special Education</a></li>
                                <li><a target="" href="@(footerLinkBaseUrl)/MDE/fam/stds/index.htm">Standards (K-12)</a></li>
                                <li><a href="@(footerLinkBaseUrl)/MDE/fam/after/slide/index.htm" target="">Summer Learning</a></li>
                            </ul>
                        </div>
                        <div class="col-md-4 col-sm-12 d-none d-sm-block noprint">
                            <ul class="noDisc">
                                <li><a href="@(footerLinkBaseUrl)/MDE/fam/tests/index.htm" target="">Testing</a></li>
                                <li><a href="@(footerLinkBaseUrl)/MDE/fam/wbwf/index.htm" target="">World's Best Workforce</a></li>
                                <li><a href="@(footerLinkBaseUrl)/MDE/about/004831" target="">Accessibility Policy</a></li>
                                <li><a href="@(footerLinkBaseUrl)/MDE/about/MDEDEV_001317" target="">Help</a></li>
                                <li><a href="https://mn.gov/portal/">Minnesota gov</a></li>
                                <li><a target="" href="@(footerLinkBaseUrl)/MDE/about/MDEDEV_001315">Privacy Statement</a></li>
                                <li><a target="" href="@(footerLinkBaseUrl)/MDE/about/MDEDEV_001316">Site Disclaimer</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-4 d-none d-sm-block">
                <div class="hfooter">Contact</div>
                <div id="contact" class="noprint justify-content-center">
                    <ul class="noDisc">
                        <li><strong>Minnesota Department of Education</strong></li>
                        <li>1500 Highway 36 West</li>
                        <li>Roseville MN 55113</li>
                        <li>651-582-8200</li>
                        <li>
                            <p><a target="" href="mailto:mde.commissioner@state.mn.us">Email the Commissioner</a></p>
                            <p><a target="" href="http://education.state.mn.us/MDE/about/map/">Directions</a></p>
                            <p><strong>Copyright 2017<br />Minnesota Department of Education</strong></p>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
