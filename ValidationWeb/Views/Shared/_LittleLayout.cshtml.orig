<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>ASDCS District Data Validation Portal</title>
    <link rel="SHORTCUT ICON" href="~/favicon.ico" />
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/tabulatorCSS")
    @Styles.Render("~/Content/jqueryUICSS")

</head>
<body>
    <div class="container-fluid layout">
        <div class="row ">
            <div class="top-banner col-12">
                <img src="~/Content/Images/mdelogo450.png" class="img-fluid mdelogo" />
                <div class="app-title clear-margin">ASDCS District Data Validation Portal</div>
            </div>
        </div>
        <div class="row noprint">
            <div class="col-12">
                <nav class="navbar navbar-expand-sm prime-blue">
                    <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#theNavBar">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="theNavBar">
                        <ul class="navbar-nav">
                            <li class="nav-item">@Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "nav-link" })</li>
                            <li class="nav-item">@Html.ActionLink("Validation Reports", "Reports", "Validation", new { area = "" }, new { @class = "nav-link" })</li>
                            <li class="nav-item">@Html.ActionLink("Data Submissions", "Submissions", "Validation", new { area = "" }, new { @class = "nav-link" })</li>
                            <li class="nav-item">@Html.ActionLink("Rule Collections", "Collections", "Rules", new { area = "" }, new { @class = "nav-link" })</li>
                            <li class="nav-item">@Html.ActionLink("Rule Editor", "Editor", "Rules", new { area = "" }, new { @class = "nav-link" })</li>
                            <li class="nav-item">@Html.ActionLink("Announcements", "Announcements", "Home", new { area = "" }, new { @class = "nav-link" })</li>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
        <div class="row layout">
            <div class="col-12">
                @RenderBody()
            </div>
        </div>
        @Html.Partial("_LittleFooter")
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
